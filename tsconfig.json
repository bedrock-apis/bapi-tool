{
    "compilerOptions": {
        "target": "ES2022",
        "lib": ["ES2024"],
        "moduleResolution": "bundler",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "strict": true,
        "skipLibCheck": true
    },
    "include": ["src/**/*"]
}
